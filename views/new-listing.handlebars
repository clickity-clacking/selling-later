<section>
  <h2>Create New Listing</h2>

  <form class="new-listing-form">
    <div>
      <label>Address</label>
      <input type="text" name="address"/>
    </div>
    <div>
    <div>
      <label>Sell Date</label>
      <input type="text" name="sell_date"/>
    </div>
    <div>
      <label>Link</label>
      <input name="post_url" />
    </div>
    <div>
      <label>Price Floor</label>
      <input type="text" name="price_floor"/>
    </div>  
    <div>
      <label>Price Ceiling</label>
      <input type="text" name="price_ceiling"/>
    </div>   
    <div>
      <label>Bedrooms</label>
      <input type="text" name="beds"/>
    </div> 
    <div>
      <label>Bathrooms</label>
      <input type="text" name="baths"/>
    </div>
    <div>
      <label>Sqft</label>
      <input type="text" name="sqft"/>
    </div>    
    <button type="submit" class="btn">Post New Listing!</button>
  </form>
</section>

<script src="/javascript/new-listing.js"></script>