<ol>
{{#each posts as |hpost|}}
  <li>
    <article>
      <div>
        <h2>{{hpost.address}}</h2>
        <span>({{hpost.post_url}})</span>
      </div>
      <div>Approx Sell Date: {{hpost.sell_date}}</div>
      <div>Price Range: {{hpost.price_floor}} - {{hpost.price_ceiling}}</div>
      <div>{{hpost.beds}}Beds, {{hpost.baths}}Ba</div>
      <div>Sqft: {{sqft}}</div>
      <div>by {{hpost.user.username}} on {{hpost.created_at}}</div>
      
    </article>
  </li>
  {{/each}}
</ol>

{{!-- search.houses copy.handlebars --}}